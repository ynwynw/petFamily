<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.petback.mapper.SubmitMapper">

    <insert id="insertReturnId" parameterType="com.example.petback.entity.Submit" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO submit (name, time, status)
        VALUES (#{name}, #{time}, #{status})
    </insert>

</mapper>